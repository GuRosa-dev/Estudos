algoritmo "soma procedimento(sub rotina)"
var
   x, y: inteiro
   
procedimento soma(A, B: inteiro)//parametros
inicio
   escreval ("Recebi o valor ", A)
   escreval ("Recebi o valor ", B)
   escreval ("A soma entre os dois é ", A+B)
fimprocedimento
   
inicio
   x <- 5
   y <- 3
   soma (x, y)
fimalgoritmo